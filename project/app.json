{
  "expo": {
    "name": "Favezilla",
    "slug": "favezilla",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "../project/assets/icon.png",
    "scheme": "favezilla",
    "splash": {
      "image": "../project/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.npetoolkit.favezilla",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.316194284018-4iucu1np3ngg7n3ngprre8iro04oe173"
        },
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "../project/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.npetoolkit.favezilla"
    },
    "web": {
      "favicon": "../project/assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "deploymentTarget": "13.0"
          },
          "android": {
            "compileSdkVersion": 31,
            "targetSdkVersion": 31,
            "buildToolsVersion": "31.0.0"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "a5a0f772-b8c9-417c-b1b4-6de4b039682e"
      }
    }
  }
}
